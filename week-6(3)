import re
n = int(input())

for _ in range(n):
    line = input()
    
    # Replace && with 'and' only if spaces on both sides
    line = re.sub(r'(?<= )&&(?= )', 'and', line)
    
    # Replace || with 'or' only if spaces on both sides
    line = re.sub(r'(?<= )\|\|(?= )', 'or', line)
    
    print(line)
